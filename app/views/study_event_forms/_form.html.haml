= form_for(@study_event_form) do |f|
  - if @study_event_form.errors.any?
    #error_explanation
      %h2
        = pluralize(@study_event_form.errors.count, "error")
        prohibited this study_event_form from being saved:
      %ul
        - @study_event_form.errors.full_messages.each do |message|
          %li= message
  .field
    = f.label :study_event_id
    %br/
    = f.text_field :study_event_id
  .field
    = f.label :study_form_id
    %br/
    = f.text_field :study_form_id
  .actions
    = f.submit
