<p id="notice"><%= notice %></p>

<h1>Listing Study Forms</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @study_forms.each do |study_form| %>
      <tr>
        <td><%= link_to 'Show', study_form %></td>
        <td><%= link_to 'Edit', edit_study_form_path(study_form) %></td>
        <td><%= link_to 'Destroy', study_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Study form', new_study_form_path %>
