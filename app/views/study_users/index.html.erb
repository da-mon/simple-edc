<p id="notice"><%= notice %></p>

<h1>Listing Study Users</h1>

<table>
  <thead>
    <tr>
      <th>Study</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @study_users.each do |study_user| %>
      <tr>
        <td><%= study_user.study.name %></td>
        <td><%= study_user.user.name %></td>
        <td><%= link_to 'Show', [study_user.study, study_user] %></td>
        <td><%= link_to 'Edit', edit_study_study_user_path(study_user.study, study_user) %></td>
        <td><%= link_to 'Destroy', [study_user.study, study_user], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Study user', new_study_study_user_path %>
