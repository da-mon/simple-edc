<p id="notice"><%= notice %></p>

<h1>Listing Study Events</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @study_events.each do |study_event| %>
      <tr>
        <td><%= link_to 'Show', study_event %></td>
        <td><%= link_to 'Edit', edit_study_event_path(study_event) %></td>
        <td><%= link_to 'Destroy', study_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Study event', new_study_event_path %>
